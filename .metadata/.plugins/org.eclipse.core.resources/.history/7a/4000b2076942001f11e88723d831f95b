package sample_application;

public class WhileSyntax {

	public static void main(String[] args) {
		
		int count = 0;
		
		while( count <10) {
			if(count == 5) {
				System.out.println("절반 했습니다");
			}
			System.out.println("반복중입니다");
			count++;
		}
		System.out.println("------------------------------------------");
		System.out.println("종료합니다");
	}
}
